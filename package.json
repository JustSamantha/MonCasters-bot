{
  "name": "habitica-bot",
  "version": "0.1.0",
  "description": "Habitica bot for the MonCasters Discord server.",
  "main": "index.js",
  "scripts": {
    "start": "npx ts-node src/index.ts",
    "start:dev": "npx ts-node-dev src/index.ts",
    "start:prod": "node dist/index.js",
    "dev": "npx nodemon ./src/index.ts",
    "build": "npx tsc",
    "watch": "npx tsc -w"
  },
  "keywords": [
    "habitica",
    "discord",
    "bot"
  ],
  "author": "Sam",
  "license": "ISC",
  "engines": {
    "npm": ">=11 <12",
    "node": ">=22 <23"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "discord.js": "^14.17.3",
    "nodemon": "^3.1.9"
  }
}
